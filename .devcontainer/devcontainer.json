{
  "name": "Java 17 Spring Boot Billing API",
  "image": "mcr.microsoft.com/devcontainers/java:17",
  "features": {
    "ghcr.io/devcontainers/features/maven:1": {
      "version": "3.9"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack",
        "pivotal.vscode-spring-boot",
        "redhat.vscode-yaml"
      ]
    }
  },
  "postCreateCommand": "mvn -version"
}

